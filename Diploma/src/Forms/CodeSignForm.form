<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="Diploma.src.Forms.CodeSignForm">
  <grid id="27dc6" binding="rootPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties>
      <maximumSize width="400" height="300"/>
      <minimumSize width="400" height="300"/>
      <preferredSize width="400" height="300"/>
    </properties>
    <border type="none"/>
    <children>
      <grid id="eac3e" binding="topPanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children/>
      </grid>
      <grid id="fd788" binding="centerPanel" layout-manager="GridLayoutManager" row-count="7" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="8c900" class="javax.swing.JLabel" binding="telNumLabel">
            <constraints>
              <grid row="1" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="0" anchor="0" fill="0" indent="0" use-parent-layout="false">
                <minimum-size width="150" height="40"/>
                <preferred-size width="150" height="40"/>
                <maximum-size width="150" height="40"/>
              </grid>
            </constraints>
            <properties>
              <font name="Source Code Pro" size="14" style="1"/>
              <horizontalAlignment value="0"/>
              <horizontalTextPosition value="0"/>
              <text value="номер телефона"/>
            </properties>
          </component>
          <vspacer id="5b026">
            <constraints>
              <grid row="6" column="0" row-span="1" col-span="1" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false">
                <minimum-size width="-1" height="75"/>
                <preferred-size width="-1" height="75"/>
                <maximum-size width="-1" height="75"/>
              </grid>
            </constraints>
          </vspacer>
          <component id="3ce92" class="javax.swing.JPasswordField" binding="codeField">
            <constraints>
              <grid row="3" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="6" anchor="0" fill="0" indent="0" use-parent-layout="false">
