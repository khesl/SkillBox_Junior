                  <minimumSize width="146" height="4"/>
                </properties>
              </component>
              <component id="75450" class="javax.swing.JButton" binding="failTimerButton">
                <constraints/>
                <properties>
                  <maximumSize width="146" height="24"/>
                  <minimumSize width="146" height="24"/>
                  <preferredSize width="146" height="24"/>
                  <text value="Выслать новый код"/>
                  <visible value="false"/>
                </properties>
              </component>
              <component id="128d9" class="javax.swing.JLabel" binding="timerCountLabel">
                <constraints/>
                <properties>
                  <text value="15s"/>
                </properties>
              </component>
            </children>
          </grid>
        </children>
      </grid>
    </children>
  </grid>
</form>
