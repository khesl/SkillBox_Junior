              </grid>
            </constraints>
            <properties>
              <horizontalAlignment value="0"/>
              <selectionEnd value="5"/>
              <selectionStart value="5"/>
              <text value="*****"/>
            </properties>
          </component>
          <component id="2b32b" class="javax.swing.JButton" binding="okButton">
            <constraints>
              <grid row="4" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="3" anchor="0" fill="0" indent="0" use-parent-layout="false">
                <minimum-size width="150" height="24"/>
                <preferred-size width="150" height="24"/>
                <maximum-size width="150" height="24"/>
              </grid>
            </constraints>
            <properties>
              <text value="Продолжить"/>
            </properties>
          </component>
          <vspacer id="c716e">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="2" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
            </constraints>
          </vspacer>
          <component id="16af6" class="javax.swing.JTextPane" binding="codeInfoTextPane">
            <constraints>
              <grid row="2" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="6" anchor="0" fill="0" indent="0" use-parent-layout="false">
                <minimum-size width="280" height="50"/>
                <preferred-size width="280" height="50"/>
                <maximum-size width="280" height="50"/>
              </grid>
            </constraints>
            <properties>
              <text value="На данный номер телефона было отправлено SMS-сообщение с кодом подтверждения. Пожалуйста, введите этот код в поле ниже:"/>
            </properties>
          </component>
          <grid id="db5c7" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
            <constraints>
              <grid row="5" column="0" row-span="1" col-span="2" vsize-policy="3" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties/>
            <border type="none"/>
            <children>
              <component id="e77fc" class="javax.swing.JLabel" binding="timerLabel">
                <constraints/>
                <properties>
                  <text value="У вас осталось для ввода кода:"/>
                </properties>
              </component>
              <component id="4b53d" class="javax.swing.JProgressBar" binding="timerBar">
                <constraints/>
                <properties>
